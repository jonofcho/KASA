<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title></title>
</head>
<body>
  <div class="container">
    <header>
      <a href="#" data-activates="slide-out" class="button-collapse"><i class="material-icons">menu</i></a>
      <nav>
        <div class="nav-wrapper">
          <a href="#" class="brand-logo"> Profile </a>
          <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li><%= link_to('Home', users_path )%></li>
            <li><%= link_to('Logout', destroy_user_session_path, :method => :delete) %></li>
          </ul>
        </div>
      </nav>
    </header>
    <ul id="slide-out" class="side-nav">
      <li><div class="userView">
        <img class="background" src="/assets/dog1.jpg">
        <a href="/users/<%=@user.id%>"><img class="circle" src="<%= @user.avatar %>"></a>
        <a href="#!name"><span class="white-text name"><%=@user.first_name%></span></a>
        <a href="#!email"><span class="white-text email"><%=@user.email%></span></a>
      </div></li>
      <li><a href="/users/edit">Edit Profile</a></li>
      <li><a href="#!">Second Link</a></li>
      <li><div class="divider"></div></li>
      <li><a class="subheader">Subheader</a></li>
      <li><a class="waves-effect" href="#!">Third Link With Waves</a></li>
    </ul>
  </div>

  <div id="wrapper" style="text-align: center">
    <div id="yourdiv">
      <a href="/profile"><img class="square" width="60%" src="<%= @member.avatar%>"></a>
      <br> <%= @member.first_name %> <%=@member.last_name%> </br>
      <%= @member.email %>
    </div>
  </div>

</body>
</html>
