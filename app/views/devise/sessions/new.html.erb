  <!DOCTYPE html>
  <html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <%@sessions_index = true%>
  <body>
    <header>
        <div class="title">Korean American Student Association</div>
        <div class="login">
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="field">
            <input type="text" name="user[email]" placeholder="E-mail">
          </div>
          <div class="field">
            <input type="password" name="user[password]" placeholder="Password">
          </div>
          <div class="action">
            <%= f.submit "Log in" %>
            <%= render "devise/shared/links" %>
          </div>
          <% end %>
        </div>
    </header>
  <div class="container">
    <div class="images">
      <img src="https://cdn.vor.us/event/316922/og/4402c73d79fb40389881ae6210dd74ff.image!jpeg.1024718.jpg.IMG_9560.jpg">
    </div>
  </div>
    <footer>
      <p>Developed by Jonathan Cho and Joey Lee</p>
    </footer>
  </body>
  </html>
